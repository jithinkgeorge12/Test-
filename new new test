import React from 'react';
import Download from './Download';

class Home extends React.Component {
  constructor(props) {
    super(props);
    this.state = {
      rawdata: [
        { date: "2024-06-25T00:00:00", location: "IRELAND" },
        { date: "2024-06-26T00:00:00", location: "IRELAND" },
        { date: "2024-06-27T00:00:00", location: "IRELAND" }
      ],
      group: ['PQR', 'ABC', 'XYZ'], // Example group array, can be based on user input
    };
  }

  handleDownloadClick = () => {
    const { rawdata, group } = this.state;
    Download.generateExcel(rawdata, group);
  }

  render() {
    return (
      <div>
        <button onClick={this.handleDownloadClick}>Generate Excel</button>
      </div>
    );
  }
}

export default Home;
